<%= form_for @venue do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :description, "Description" %>
  <%= f.text_area :description %>
  <%= f.label :address_one, "Address" %>
  <%= f.text_field :address_one %>
  <%= f.label :city, "City" %>
  <%= f.text_field :city %>
  <%= f.label :province, "Province" %>
  <%= f.text_field :province %>
  <%= f.label :country, "Country" %>
  <%= f.text_field :country %>
  <%= f.label :num_of_seats, "Capacity" %>
  <%= f.number_field :num_of_seats %>
  <%= f.button %>
<% end %>
