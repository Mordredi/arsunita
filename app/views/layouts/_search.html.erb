<div id="search-results">
  <h3>Users</h3>
  <% @users.each do |user| %>
    <%= link_to user.full_name, user_path(user) %>
  <% end %>
  <h3>Events</h3>
  <% @events.each do |event| %>
    <%= link_to event.name, event_path(event) %>
  <% end %>
  <h3>Venues</h3>
  <% @venues.each do |venue| %>
    <%= link_to venue.name, venue_path(venue) %>
  <% end %>
</div>